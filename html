    <script>
        // Beat maker functionality
        const beatPads = document.querySelectorAll('.beat-pad');
        const sounds = [
            'sounds/kick.mp3',
            'sounds/snare.mp3',
            'sounds/hihat.mp3',
            'sounds/clap.mp3',
            'sounds/bass.mp3',
            'sounds/cymbal.mp3',
            'sounds/perc.mp3',
            'sounds/vox.mp3'
        ];
        
        // In a real implementation, we would preload audio files
        beatPads.forEach((pad, index) => {
            pad.addEventListener('mousedown', () => {
                pad.classList.add('active');
                // This would play the corresponding sound
                // new Audio(sounds[index]).play();
                setTimeout(() => {
                    pad.classList.remove('active');
                }, 200);
            });
        });
        
        // Karaoke functionality
        const recordBtn = document.querySelector('.control-btn:nth-child(1)');
        const playbackBtn = document.querySelector('.control-btn:nth-child(2)');
        const lyrics = document.querySelector('.lyrics');
        const lines = lyrics.querySelectorAll('p')[0].innerHTML.split('<br>');
        let currentLine = 0;
        
        recordBtn.addEventListener('click', () => {
            if(recordBtn.innerHTML.includes('Start')) {
                recordBtn.innerHTML = '<i class="fas fa-stop"></i> Stop Recording';
                recordBtn.style.background = 'var(--accent)';
                startLyricsHighlight();
            } else {
                recordBtn.innerHTML = '<i class="fas fa-microphone"></i> Start Recording';
                recordBtn.style.background = 'var(--primary)';
                clearInterval(lyricsInterval);
            }
        });
        
        playbackBtn.addEventListener('click', () => {
            playbackBtn.innerHTML = '<i class="fas fa-pause"></i> Pause';
            setTimeout(() => {
                playbackBtn.innerHTML = '<i class="fas fa-play"></i> Playback';
            }, 3000);
        });
        
        function startLyricsHighlight() {
            currentLine = 0;
            updateLyricsDisplay();
            
            lyricsInterval = setInterval(() => {
                currentLine = (currentLine + 1) % lines.length;
                updateLyricsDisplay();
            }, 5000);
        }
        
        function updateLyricsDisplay() {
            let content = '';
            for (let i = 0; i < lines.length; i++) {
                if (i === currentLine) {
                    content += `<span class="current-line">${lines[i]}</span><br>`;
                } else {
                    content += `${lines[i]}<br>`;
                }
            }
            lyrics.querySelector('p').innerHTML = content;
        }
        
        // Phaser game for VScript storyline
        const config = {
            type: Phaser.AUTO,
            width: 800,
            height: 500,
            parent: 'game-container',
            scene: {
                preload: preload,
                create: create,
                update: update
            }
        };
        
        const game = new Phaser.Game(config);
        
        function preload() {
            // In a real implementation, we'd load game assets
            // this.load.image('background', 'images/game-bg.jpg');
            // this.load.spritesheet('character', 'images/player.png', { frameWidth: 32, frameHeight: 48 });
        }
        
        function create() {
            // Create game background
            // this.add.image(400, 250, 'background');
            
            // Add storyline text
            const storyText = this.add.text(400, 150, 'LEVEL UP: RHYTHM QUEST', {
                fontSize: '36px',
                fill: '#ffd166',
                fontFamily: 'Montserrat',
                stroke: '#000',
                strokeThickness: 4
            }).setOrigin(0.5);
            
            const instruction = this.add.text(400, 250, 'Complete curriculum challenges to unlock new beats and advance your rap career!', {
                fontSize: '20px',
                fill: '#fff',
                fontFamily: 'Poppins',
                wordWrap: { width: 600 },
                align: 'center'
            }).setOrigin(0.5);
            
            const startButton = this.add.rectangle(400, 350, 200, 60, 0x4cc9f0)
                .setInteractive()
                .on('pointerdown', () => {
                    // Start the game
                });
                
            this.add.text(400, 350, 'BEGIN JOURNEY', {
                fontSize: '24px',
                fill: '#fff',
                fontFamily: 'Montserrat'
            }).setOrigin(0.5);
        }
        
        function update() {
            // Game update logic
        }
        
        // Animated background
        document.addEventListener('mousemove', (e) => {
            const x = e.clientX / window.innerWidth;
            const y = e.clientY / window.innerHeight;
            
            document.body.style.backgroundPosition = `${x * 100}% ${y * 100}%`;
        });
    </script>
</body>
</html>
